{
    "name": "yeeraf/laravel-document-numberer",
    "description": "A Laravel package for create document running number",
    "type": "library",
    "require": {
        "php": "^8.0"
    },
    "authors": [
        {
            "name": "Suphasit",
            "email": "suphasit.s@yeeraf.co.th"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yeeraf\\DocumentNumberer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yeeraf\\DocumentNumberer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yeeraf\\DocumentNumberer\\PackageServiceProvider"
            ],
            "aliases": {
                "DocumentNumberer": "Yeeraf\\DocumentNumberer\\src\\Facades\\DocumentNumbererFacade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
